<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="cron_sync_status_to_prestashop" model="ir.cron">
            <field name="name">sync status to prestashop</field>
            <field name="model_id" ref="model_stock_website_order"/>
            <field name="state">code</field>
            <field name="code">env['stock.website.order'].sync_status_to_prestashop()</field>
            <field name="interval_number">7</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>
