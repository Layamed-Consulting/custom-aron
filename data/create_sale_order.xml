<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Cron Job: Export Products to PrestaShop every 5 minutes -->
        <record id="cron_create_batch_sale_orders_dynamic" model="ir.cron">
            <field name="name">Create sale order directly</field>
            <field name="model_id" ref="model_stock_website_order"/>
            <field name="state">code</field>
            <field name="code">env['stock.website.order'].action_create_batch_sale_orders_dynamic()</field>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>