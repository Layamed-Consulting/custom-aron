<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="cron_update_invoice_names" model="ir.cron">
            <field name="name">Change invoice name</field>
            <field name="model_id" ref="model_stock_website_order"/>
            <field name="state">code</field>
            <field name="code">env['stock.website.order'].cron_update_invoice_names()</field>
            <field name="interval_number">30</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>